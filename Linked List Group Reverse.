class Solution {
  public:
    Node *reverseKGroup(Node *head, int k) {
        // code here
        Node*temp=head;
        Node*temp2=head;
        
        while(temp!=nullptr){
                stack<int>st;
            for(int i=1;i<=k;i++){
                if(temp==nullptr){
                    break;
                }
                st.push(temp->data);
                temp=temp->next;
            }
            
            while(!st.empty()){
                int val=st.top();
                st.pop();
                temp2->data=val;
                temp2=temp2->next;
            }
            //temp=temp->next;
        }
        return head;
    }
};
