class Solution {
  public:
    vector<string> binstr(int &n) {
        // code here
        vector<string> ans((1 << n), string(n, '0'));
        for(int i = 0; i < (1 << n); i++) {
            for(int j = 0; j < n; j++) 
                if(i & (1 << j)) ans[i][(n - j - 1)] = '1';
        }
        return ans;
    }
};
