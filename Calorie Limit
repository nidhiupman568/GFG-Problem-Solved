#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int d[1000],sum=0;
	    int a,b,c=0;
	    cin>>a>>b;
	    for(int i=0;i<a;i++){
	        cin>>d[i];
	        sum+=d[i];
	        if(sum<=b){
	            c++;
	        }
	    }
	    if(c>0){
	        cout<<c<<endl;
	    }
	    else{
	        cout<<"0"<<endl;
	    }
	}

}
