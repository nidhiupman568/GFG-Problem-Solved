class Solution {
public:
    int maximumProfit(vector<int> &prices) {
        int maxProfit = 0;
        int a = prices[0];
        for(int i = 1;i<prices.size();i++){
            if(prices[i] < prices[i-1]){
                maxProfit += prices[i-1]-a;
                a = prices[i];
            }
        }
        maxProfit += prices[prices.size()-1] - a;
        return maxProfit;
    }
};

