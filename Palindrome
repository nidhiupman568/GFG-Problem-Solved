class Solution
{
	public:
		string is_palindrome(int n)
		{
		    // Code here.
		      int rev=0,p=n;

              while (n!=0){

                  int digit=n%10;

                  rev=rev *10+digit;

                  n=n/10;

              }

              if(p==rev){

                   return  "Yes";

              }

              else {

                    return  "No";
              }
		}
};
